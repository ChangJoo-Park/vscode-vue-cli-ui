{
    "name": "vscode-vue-ui",
    "displayName": "vscode-vue-ui",
    "description": "Vue.js UI on vscode",
    "version": "0.0.1",
    "publisher": "changjoo-park",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openVueUI"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.openVueUI",
                "title": "Open Vue UI"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
